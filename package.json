{
  "name": "smartroster",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon src/server/server.mjs",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "apidoc": "apidoc -i src/server/services/ -o docs/apidoc/",
    "apidoc:serve": "npx serve docs/apidoc"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@icon-park/vue-next": "^1.4.2",
    "@iconify-json/ic": "^1.2.2",
    "@types/node": "^22.14.0",
    "animate.css": "^4.1.1",
    "axios": "^1.8.2",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "element-plus": "^2.9.7",
    "express-http-proxy": "^2.1.1",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.0",
    "nodemon": "^3.1.9",
    "pinia": "^2.3.1",
    "unocss": "^65.5.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@types/supertest": "^6.0.3",
    "@unocss/preset-icons": "66.1.0-beta.6",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "apidoc": "^1.2.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "eslint": "^9.21.0",
    "express": "^4.21.2",
    "only-allow": "^1.2.1",
    "supertest": "^7.1.0",
    "typescript": "~5.6.3",
    "vite": "^6.2.1",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
